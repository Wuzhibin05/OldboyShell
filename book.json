{
    "title" : "金证财富KACE项目手册",
    "description" : "金证财富KACE项目手册",
    "author" : "Kace Team",
    "language" : "zh-hans",
    "styles": {
        "website": "assets/styles/website.css",
        "pdf": "assets/styles/pdf.css"
    },
    "structure": {
        "readme": "README.md"
    },
    "pdf": {
		"pageNumbers":true,
		"fontSize": 12,
		"fontFamily": "Arial",
		"paperSize": "a4",
        "margin": {
            "left": 0,
            "top": 0,
            "right": 0,
            "bottom": 0
        }
    },
    "plugins": [
	"-search",
        "-sharing","sharing-plus", 
        "prism",
        "-highlight",
        "prism-themes",
		"copy-code-button",
        "back-to-top-button",
        "chapter-fold", 
		"-lunr", "-search", "search-pro",
        "splitter", 
        "-tbfed-pagefooter",
        "anchor-navigation-ex",
        "emphasize",
        "advanced-emoji",
        "custom-js-css",
        "expandable-chapters"
    ],
    "pluginsConfig": {
        "sharing": {
            "douban": true,
            "facebook": false,
            "google": true,
            "hatenaBookmark": false,
            "instapaper": false,
            "line": true,
            "linkedin": true,
            "messenger": false,
            "pocket": false,
            "qq": false,
            "qzone": true,
            "stumbleupon": false,
            "twitter": false,
            "viber": false,
            "vk": false,
            "weibo": true,
            "whatsapp": false,
            "all": [
                "douban", "facebook", "google", "hatenaBookmark", 
                "instapaper", "linkedin","twitter", "weibo", 
                "messenger","qq", "qzone","viber","vk","weibo",
                "pocket", "stumbleupon","whatsapp"
            ]
        },
        "tbfed-pagefooter": {
            "copyright":"Copyright &copy szkingdom.com 2020",
            "modify_label": "2020-4-21：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
        "anchor-navigation-ex":{
            "showLevel": false,
			"associatedWithSummary": false,
			"printLog": false,
			"multipleH1": true
        },
        "prism": {
			"css": [
				"prismjs/themes/prism-okaidia.css"
			]
        }
    }  
}